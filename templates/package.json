{
  "name": "<%= packageName %>",
  "version": "0.0.0",
  "scripts": {
    "serve" : "static",
    "prepublish": "npm run build",
    "watch": "watchy -sw src -- npm run build",
    "build": "traceur --dir src/ out/ --modules=commonjs --sourcemap"
  },
  "dependencies": {
    "watchy": "~0.4.4",
    "traceur": "~0.0.42",
    "node-static": "~0.7.3",
    "es6-module-loader": "~0.6.0"
  }
}
